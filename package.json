{
  "name": "package_template",
  "version": "1.0.0",
  "description": "ðŸš€ Package template",
  "scripts": {
    "test": "jest",
    "build:dist": "rimraf dist && rollup -c",
    "build:docs": "typedoc",
    "build": "npm run build:dist && npm run build:docs",
    "prepack": "npm run build",
    "lint": "eslint src/*",
    "prettify": "prettier --write src/**/*"
  },
  "packageManager": "npm@9.1.0",
  "main": "./index.js",
  "module": "./index.mjs",
  "type": "commonjs",
  "types": "./index.d.ts",
  "typings": "./index.d.ts",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./index.js",
      "import": "./index.mjs",
      "types": "./index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexXanderGrib/package-template.git"
  },
  "keywords": [],
  "files": [
    "dist/",
    "src/",
    "coverage/",
    "examples/",
    "index.js",
    "index.mjs",
    "index.d.ts",
    "README.md",
    "CHANGELOG.md",
    "CONTRIBUTING.md",
    "LICENSE.txt",
    "SECURITY.md",
    "CODE_OF_CONDUCT.md"
  ],
  "author": "Alexander G <me@alexxgrib.me> (https://alexxgrib.me/)",
  "maintainers": [
    "Alexander G <me@alexxgrib.me> (https://alexxgrib.me/)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexXanderGrib/package-template/issues"
  },
  "homepage": "https://github.com/AlexXanderGrib/package-template#readme",
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.1",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-unicorn": "^49.0.0",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.5.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-prettier": "^4.1.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.3",
    "typedoc-plugin-markdown": "^3.17.1",
    "typedoc-plugin-missing-exports": "^2.1.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16"
  }
}
